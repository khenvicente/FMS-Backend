{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "routes/**/*",
    "models/**/*",
    "controllers/**/*",
    "config/**/*",
    "migration/**/*"
  ]
}
